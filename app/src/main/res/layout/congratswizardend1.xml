<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/content_container"
	android:background="@drawable/noise_texture_light_shape">


	<RelativeLayout
		android:id="@+id/button___normal_ek13"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/microsoftteams_image__23__2_ek2"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="30dp"
		android:background="@drawable/background_color_ek3_shape">


		<TextView
			android:id="@+id/button_label_ek13"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="13dp"
			android:layout_marginTop="10dp"
			android:layout_marginEnd="13dp"
			android:layout_marginBottom="10dp"
			android:text="@string/button_label_ek13_string"
			android:textColor="@color/button_label_color"
			android:textSize="18sp"
			android:textStyle="bold" />

		<ImageView
			android:id="@+id/path_ek3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginStart="5dp"
			android:layout_marginEnd="10dp"
			android:layout_toRightOf="@+id/button_label_ek13"
			android:contentDescription="path_ek3"
			android:src="@drawable/path_ek3" />


	</RelativeLayout>

	<TextView
		android:id="@+id/welcome_john__sign_in_to_continue_ek10"
		android:layout_width="318dp"
		android:layout_height="159dp"
		android:layout_gravity="center_horizontal|center"
		android:layout_marginLeft="38dp"
		android:layout_marginTop="100dp"
		android:gravity="center"
		android:text="@string/welcome_john__sign_in_to_continue_ek10_string"
		android:textColor="@color/_bg__sign_in_1_ek2_color"
		android:textSize="22sp"
		android:textStyle="bold" />


	<RelativeLayout
		android:id="@+id/component___home_indicator_ek7"
		android:layout_width="150dp"
		android:layout_height="6dp"
		android:layout_centerHorizontal="true"
		android:layout_alignParentBottom="true"
		android:layout_marginBottom="10dp"
		android:background="@drawable/dark_color_13_ek7_shape">

	</RelativeLayout>

	<ImageView
		android:id="@+id/microsoftteams_image__23__2_ek2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/welcome_john__sign_in_to_continue_ek10"
		android:layout_marginStart="69dp"
		android:layout_marginTop="30dp"
		android:contentDescription="microsoftteams_image__23__2_ek2"
		android:src="@drawable/microsoftteams_image__23__2_ek2" />

			</RelativeLayout>




